package components

templ Head(title, description string) {
	@elements.Head() {
		<meta charset="UTF-8"/>
		<meta name="viewport" content="width=device-width, initial-scale=1.0"/>
		<meta name="description" content={ description }/>
		<title>{ title }</title>
		@elements.Script("https://cdn.tailwindcss.com")
		{ children... }
	}
}
